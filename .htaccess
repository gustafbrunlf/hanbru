RewriteEngine On
RewriteCond %{HTTPS} off
RewriteRule ^ https://hannabrunlof.se%{REQUEST_URI} [NE,L,R]

# BEGIN WordPress
<IfModule mod_rewrite.c>
RewriteEngine On
RewriteBase /hannabrunlof/
RewriteRule ^index\.php$ - [L]
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule . /hannabrunlof/index.php [L]
</IfModule>

# END WordPress